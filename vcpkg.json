{
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
    "name": "dicom-viewer",
    "version": "0.3.0",
    "description": "High-performance DICOM Viewer with 3D Volume Rendering",
    "homepage": "https://github.com/kcenon/dicom_viewer",
    "license": "MIT",
    "dependencies": [
        {
            "name": "qtbase",
            "default-features": false,
            "features": ["widgets", "gui", "opengl"]
        },
        {
            "name": "vtk",
            "default-features": false,
            "features": ["qt", "opengl"]
        },
        {
            "name": "itk",
            "default-features": false,
            "features": ["vtk"]
        },
        "spdlog",
        "fmt",
        "nlohmann-json",
        "gtest"
    ],
    "builtin-baseline": "84bab45d415d22042bd0b9081aea57f362da3f35"
}
